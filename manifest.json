{
  "manifest_version": 2,
  "name": "Toggle",
  "version": "1.1",
  "description": "",
  "permissions": ["activeTab", "storage", "webNavigation", "<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/toggle16.png",
      "48": "icons/toggle48.png",
      "128": "icons/toggle128.png"
    }
  },
  "icons": {
    "16": "icons/toggle16.png",
    "48": "icons/toggle48.png",
    "128": "icons/toggle128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}